{{define "base"}}
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<meta charset="UTF-8">
      <title>{{.Title}}</title>
			<meta http-equiv="X-UA-Compatible" content="IE=edge">
			<meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
      <link rel="preconnect" href="https://fonts.googleapis.com">
      <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
      <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,400;0,600;0,700;1,300&display=swap" rel="stylesheet">
      <link rel="stylesheet" href="public/css/vendor/normalize.min.css">
      <link rel="stylesheet" type="text/css" href="https://unpkg.com/notie/dist/notie.min.css">
      <link rel="stylesheet" type="text/css" href="/public/css/main.min.css">
			
      <style>
        .notie-container {
          font-size: 1em;
          z-index: 9;
        }
      </style>

			{{block "css" .}}{{end}}

      {{ if not .Production }}
        <script src="https://cdn.tailwindcss.com"></script>
        <script>
          tailwind.config = {
            content: [
              './templates/*.html',
              './public/js/*.js'
            ],
            theme: {
                extend: {
                container: {
                    center: true,
                    padding: {
                    DEFAULT: '1rem',
                    md: '0'
                  }
                }
              }
            },
            plugins: []
          }
        </script>
      {{end}}
		</head>
		<body>
			<header id="site-navigation-navbar" class="fixed flex items-center h-16 w-full transition-shadow" style="background-color: var(--primary-color)">
        <div class="container flex items-center justify-between md:justify-start">
    
          <a href="/" class="brand-logo flex flex-row items-center" aria-label="Wilkinson Innovations">
            <svg class="mr-4" xmlns="http://www.w3.org/2000/svg" xml:space="preserve" fill-rule="evenodd" stroke-linejoin="round" stroke-miterlimit="2" clip-rule="evenodd" height="32" viewBox="0 0 938 734"><g id="wi-icon"><clipPath id="_clip1"><path id="w" d="M164.163,55.412l100.302,677.55l-160.4,-0l-100.302,-677.55l160.4,0Zm305.07,340.158l49.658,337.392l-160.52,-0l-49.658,-337.392l160.52,-0Zm209.314,-340.158l100.302,677.55l-160.399,-0l-100.302,-677.55l160.399,0Z"/></clipPath><g clip-path="url(#_clip1)"><path fill="var(--secondary-color)" d="M.998 52.647H781.615V735.7270000000001H.998z"/></g><g id="i" ><clipPath id="_clip2"><path id="bottom" d="M935.735,292.119l-116.152,0l-33.654,-240.599l116.152,0l33.654,240.599Z"/></clipPath><g clip-path="url(#_clip2)"><path fill="var(--secondary-color)" d="M783.164 48.755H938.5V294.885H783.164z"/></g><clipPath id="_clip3"><path id="top" d="M899.783,34.927l-116.151,0l-5.527,-33.186l116.152,0l5.526,33.186Z"/></clipPath><g clip-path="url(#_clip3)"><path fill="#f3342f" d="M775.34-1.024H902.549V37.693H775.34z"/></g></g></g></svg>
            <span class="font-bold min-[320px]:text-base min-[375px]:text-xl min-[413px]:text-2xl whitespace-nowrap">Wilkinson Innovations</span>
          </a>
    
          <input id="site-navigation-navbar-toggle" class="hidden" type="checkbox">
          <label for="site-navigation-navbar-toggle" class="cursor-pointer md:hidden"><span></span></label>
    
          <div id="site-navigation-navbar-nav-links" class="max-h-0 md:max-h-full overflow-hidden absolute md:relative top-16 md:top-0 left-0 flex md:items-center md:justify-between m-0 md:ml-6 w-full shadow-lg md:shadow-none" style="background-color: var(--primary-color);">
            <nav class="block md:flex items-center justify-between w-full">
              <div>
                <a href="/#features" class="features block md:inline-block mb-1 md:mb-0 pl-4 md:px-4 py-2">Features</a>
                <a href="/about" class='about block md:inline-block mb-1 md:mb-0 pl-4 md:px-4 py-2 {{if eq .Title "About | Wilkinson Innovations"}}active{{end}}'>About</a>
              </div>
              <a href="/contact" class='contact block md:inline-block mb-4 md:mb-0 pl-4 md:px-4 py-2  {{if eq .Title "Contact Us | Wilkinson Innovations"}}active{{end}}'>Contact</a>
            </nav>
          </div>
        </div>
      </header>

			{{block "content" .}}{{end}}

			<footer id="site-navigation-footbar" class="pt-20">
        <div class="container flex">
          <div class="flex flex-col md:grid grid-cols-2 grid-flow-col">
